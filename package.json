{
	"name": "sg-finland-frontend",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"test": "npm run test:integration && npm run test:unit",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"lint:fix": "pnpm lint --fix",
		"format": "prettier --write --plugin prettier-plugin-svelte .",
		"test:integration": "playwright test",
		"test:unit": "vitest",
		"generate:strapi": "npx strapi-generate-types quickgen http://localhost:1337 --path ./src/types/generated --file-name strapi.ts"
	},
	"devDependencies": {
		"@playwright/test": "^1.41.2",
		"@poppanator/sveltekit-svg": "^4.2.1",
		"@sveltejs/adapter-auto": "^3.1.1",
		"@sveltejs/adapter-netlify": "^4.1.0",
		"@sveltejs/kit": "^2.5.0",
		"@sveltejs/vite-plugin-svelte": "^3.0.2",
		"@twicpics/components": "^0.27.1",
		"@types/qs": "^6.9.11",
		"@typescript-eslint/eslint-plugin": "^6.20.0",
		"@typescript-eslint/parser": "^6.20.0",
		"autoprefixer": "^10.4.17",
		"clsx": "^2.1.0",
		"cssnano": "^6.0.3",
		"dayjs": "^1.11.10",
		"eslint": "^8.56.0",
		"eslint-config-prettier": "^8.10.0",
		"eslint-plugin-svelte": "^2.35.1",
		"postcss": "^8.4.33",
		"prettier": "^3.2.4",
		"prettier-plugin-svelte": "^3.1.2",
		"prettier-plugin-tailwindcss": "^0.5.11",
		"qs": "^6.11.2",
		"svelte": "^4.2.9",
		"svelte-check": "^3.6.3",
		"svelte-markdown": "^0.4.1",
		"svelte-time": "^0.8.2",
		"tailwind-merge": "^1.14.0",
		"tailwindcss": "^3.4.1",
		"tslib": "^2.6.2",
		"typescript": "^5.3.3",
		"vite": "^5.0.12",
		"vitest": "^1.2.2"
	},
	"type": "module",
	"dependencies": {
		"@auth/sveltekit": "^0.14.0",
		"lucide-svelte": "^0.292.0",
		"tailwind-variants": "^0.1.20"
	}
}
